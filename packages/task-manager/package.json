{
  "name": "@opuscapita/fsm-workflow-task-manager",
  "version": "1.0.0",
  "description": "Task manager for FSM workflow",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "lint": "eslint src",
    "test": "rimraf ./.nyc_output ./coverage && cross-env nyc --reporter=lcov --reporter=text mocha --reporter=mocha-junit-reporter --require config/mocha-setup.js --recursive 'src/**/*.spec.js'",
    "prepack": "npm run build"
  },
  "repository": "https://github.com/OpusCapita/fsm/tree/master/packages/task-manager",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "Daniel Zhitomirsky <dzhitomirsky@scand.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpusCapita/fsm/issues"
  },
  "homepage": "https://github.com/OpusCapita/fsm/tree/master/task-manager#readme",
  "dependencies": {
    "bluebird": "3.5.0"
  },
  "devDependencies": {
    "@opuscapita/fsm-workflow-core": "1.0.0",
    "@opuscapita/npm-scripts": "2.0.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "5.0.5",
    "eslint": "3.19.0",
    "eslint-config-opuscapita": "1.0.9",
    "eslint-plugin-react": "6.10.3",
    "mocha": "3.5.0",
    "mocha-junit-reporter": "1.13.0",
    "nyc": "11.1.0",
    "rimraf": "2.6.1",
    "webpack": "3.5.5"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "**/*.spec.js"
    ],
    "check-coverage": true,
    "lines": 70,
    "statements": 70,
    "functions": 70,
    "branches": 70
  }
}
