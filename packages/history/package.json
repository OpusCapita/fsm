  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "umzug": "2.1.0"
  },
  "peerDependencies": {
    "sequelize": "~3 || ~4"
  },
  "devDependencies": {
    "@opuscapita/npm-scripts": "2.0.0",
    "babel-eslint": "7.2.3",
    "body-parser": "1.18.2",
    "chai": "4.1.2",
    "cross-env": "5.1.3",
    "eslint": "3.19.0",
    "eslint-config-opuscapita": "1.0.7",
    "eslint-plugin-react": "6.10.3",
    "express": "4.16.2",
    "helmet": "3.10.0",
    "mocha": "5.0.0",
    "mocha-junit-reporter": "1.13.0",
    "mysql2": "1.5.2",
    "nyc": "11.1.0",
    "sequelize": "4.33.1",
    "sqlite3": "3.1.13",
    "rimraf": "2.6.1"
  },
  "nyc": {
    "per-file": true,
